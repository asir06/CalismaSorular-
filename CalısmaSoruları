package CalısmaSoruları;
import java.util.*;

public class Soru12 {

	public static void main(String[] args) {
		// Girilen üç sayının en büyüğünü bulan programı yazınız.
		Scanner giris=new Scanner(System.in); 
		int sayi1,sayi2,sayi3;
		System.out.println("Birinci Sayıyı Giriniz :");
		sayi1=giris.nextInt();
		System.out.println("İkinci Sayıyı Giriniz :");
		sayi2=giris.nextInt(); 
		System.out.println("Üçüncü Sayıyı Giriniz :");
		sayi3=giris.nextInt();
		int Biriki=Math.max(sayi1,sayi2); 
		int ikiuc=Math.max(Biriki, sayi3);
		System.out.println("En Büyük Sayı :"+ikiuc);

		int birikis=Math.min(sayi1, sayi2);
		int ikiucs=Math.min(birikis, sayi3);
		System.out.println("En Küçük Sayı :"+ikiucs);

	}

}
