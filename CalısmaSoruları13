package CalısmaSoruları;
public class Soru13 {
	public static void main(String[] args) {
		//1 ile 1000 arasındaki asal sayıları bulan programı yazınız.
		int sayi=0;
		for(int i=2; i<1000; i++)
		{
			sayi=0;
			for(int j=2; j<=i/2; j++)
			{
				if(i%j==0)
				{
					sayi++;
				}
			}
			if(sayi==0)
			{
				System.out.print(i+" ");
			}
		}

	}

}
